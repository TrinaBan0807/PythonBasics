      "source": [
        "n = int(input(\"Enter the number: \"))\n",
        "\n",
        "def print_patterns(n):\n",
        "  for i in range(n):\n",
        "    for j in range(i+1):\n",
        "      print(\"* \",end=\"\")\n",
        "    print()\n",
        "\n",
        "  for i in range(n,0,-1):\n",
        "    for j in range(i-1):\n",
        "      print(\"* \",end=\"\")\n",
        "    print()\n",
        "\n",
        "print_patterns(n)\n"
      ],
